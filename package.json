  "description": "Find and warn your contacts to prevent covid-19 spread",
  "name": "CasContact",
  "private": true,
  "scripts": {
    "check-types": "tsc",
    "checks": "run-s -nc lint check-types test",
    "clean": "rimraf dist/*",
    "copy": "copyfiles ./favicon.ico ./dist",
    "dist": "npm run clean && npm run copy && REACT_WEBPACK_ENV=dist CLIENT_VERSION=$CLIENT_VERSION webpack --bail",
    "fix:lint": "eslint . --ext js,jsx,ts,tsx --fix",
    "i18n": "babel --config-file ./i18n.babelrc.js 'src/**/*.{js,jsx,ts,tsx}' > /dev/null",
    "lint": "run-s lint:*",
    "lint:config": "pjv && ./jsonlint.sh package.json",
    "lint:js": "eslint . --ext js,json5,jsx,ts,tsx",
    "lint:jscolor": "./check-color-config.sh",
    "serve": "REACT_WEBPACK_ENV=dev node server.js",
    "serve:dist": "REACT_WEBPACK_ENV=dist node server.js",
    "start": "REACT_WEBPACK_ENV=dev CLIENT_VERSION=$CLIENT_VERSION node server.js",
    "test": "karma start",
    "test:node": "mocha test/node/*.{js,ts} --require ts-node/register",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "version": "0.0.1"
